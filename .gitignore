# Start: Based on research
# Node modules and dependencies
node_modules/
npm-debug.log
yarn.lock
pnpm-lock.yaml

# Laravel vendor and caches
/vendor
/bootstrap/cache/*
!bootstrap/cache/.gitignore  # Allow directory but ignore contents

# Laravel storage (keep structure but ignore files)
storage/*
!storage/app/
!storage/framework/
!storage/keys/
!storage/logs/
!storage/framework/cache/
!storage/framework/sessions/
!storage/framework/views/
