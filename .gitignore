# Environment files (DO NOT COMMIT SECRETS)
.env
.env.backup
.env.production
/.phpunit.result.cache

# IDE and system files
/.idea/
/.vscode/
/.DS_Store
Thumbs.db

# Logs and caches
*.log
storage/logs/
storage/framework/cache/data/

# Composer dependencies
composer.lock

# Vue.js build files (ignore development builds)
public/hot
public/storage
/public/mix-manifest.json
/public/build/
public/js/*.map
public/css/*.map

# Database files (if using SQLite)
*.sql
*.sqlite

# Docker (if applicable)
.docker/
docker-compose.override.yml  # Keep the base docker-compose.yml
docker-compose.dev.yml  # Ignore dev-specific configs

# Miscellaneous
*.swp
*.swo

